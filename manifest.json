{
  "manifest_version": 3,
  "name": "Pawfect Pics",
  "version": "1.1",
  "description": "View pictures of your favorite breed of dogs!",
  "permissions": ["storage"],
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  }
}
